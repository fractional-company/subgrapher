{
  "name": "@fractional-company/subgrapher",
  "version": "0.0.5",
  "description": "Simple utility for querying popular Subgraphs.",
  "main": "./lib/src/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./lib/",
    "cm": "cz",
    "coverage": "codecov",
    "lint": "eslint ./src/ --fix",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "watch": "npx tsc --watch",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fractional-company/subgrapher.git"
  },
  "license": "MIT",
  "author": {
    "name": "CryptoSamurai",
    "url": "https://fractional.art/"
  },
  "engines": {
    "node": ">=14.0"
  },
  "keywords": [
    "dex",
    "lp",
    "subgraph",
    "uniswap",
    "sushiswap",
    "blocks"
  ],
  "bugs": {
    "url": "https://github.com/fractional-company/subgrapher.git/issues"
  },
  "homepage": "https://github.com/fractional-company/subgrapher.git#readme",
  "devDependencies": {
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "codecov": "^3.8.3",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "semantic-release": "^19.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "bignumber.js": "^9.0.2",
    "date-fns": "^2.28.0",
    "graphql-request": "^4.3.0",
    "ts-jest": "^28.0.4"
  }
}
